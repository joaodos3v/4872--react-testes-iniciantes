{
  "name": "checklist",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "concurrently 'npm run start:api' 'npm run dev'",
    "start:api": "npx json-server db.json --port 3001",
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:low-cost": "jest --runInBand",
    "test:coverage": "jest --coverage",
    "test:coverage-all": "jest --coverage --collectCoverageFrom='src/**/*.{js,jsx}'"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@eslint/js": "^9.22.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "babel-jest": "^30.0.5",
    "concurrently": "^9.2.0",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "json-server": "^1.0.0-beta.3",
    "prettier": "^3.6.2",
    "vite": "^6.3.1"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
